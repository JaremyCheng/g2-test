_initEvents(); //初始化事件
_initContainer(); //初始化容器
_initColumns(); //初始化columns
_initBody(); //初始化Body区域
_initCols(); //初始化columns部分状态，如锁定

loadData; //加载数据
clearData： //清除數據
setUnit; //设置unit
createUnit; //创建unit
bindUnit; //绑定unit
unbindUnit; //解绑unit

getTrs; //獲取所有的tr
getTr; //由index獲取某個tr
getTrByUid; //由uid獲取tr
getRow; //由index獲取row對象
getRowByUid; //由uid獲取row對象

lock; //鎖定
unlock; //解鎖
lockCol; //鎖定列
unlockCol; //解鎖列

getView; //獲取視圖
changeColView; //修改列試圖
getColumn; //獲取列

renderTr; //渲染tr
renderData; //渲染data
afterRender; //渲染之後處理
-------------------------------
_initMainSize; //初始化mian view大小
_initViewPos; //初始化view位置，如y軸滾動條位置
_initDataAreaEvent; //初始化數據區域事件
_initTrEvents; //初始化Tr的事件


_initSelected; //初始化選中
select; //選中行
refreshSelected; //刷新選中

beginEdit; //開始編輯
endEdit; //結束編輯
removeRow; //刪除行
deleteRow; //標記刪除狀態，暫時刪除爲直接後臺刪除，這部分以後再追加
insertRow; //插入行
appendRow; //最後追加行
prependRow; //最前追加行
_insertTr; //插入tr

filter; //過濾
sort; //排序

doResize; //重寫resize
_initMainSize; //////
_resizeHead; //初始化head大小